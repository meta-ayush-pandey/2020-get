<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from RestApiService WSDL service -->

  <service name="RestApi" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://soapWebServices.com"/>
      <parameter name="wsdlServiceElement" value="RestApiService"/>
      <parameter name="schemaQualified" value="http://soapWebServices.com"/>
      <parameter name="wsdlServicePort" value="RestApi"/>
      <parameter name="className" value="com.soapWebServices.RestApiSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="RestApi"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="area" qname="operNS:area" xmlns:operNS="http://soapWebServices.com" returnQName="retNS:areaReturn" xmlns:retNS="http://soapWebServices.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:sideA" xmlns:pns="http://soapWebServices.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:sideB" xmlns:pns="http://soapWebServices.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:sideC" xmlns:pns="http://soapWebServices.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="area"/>

  </service>
</deployment>
